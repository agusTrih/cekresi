{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["plugin:react/recommended", "plugin:react/jsx-runtime", "airbnb"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "plugin:react/jsx-runtime": 0,
    "react/react-in-jsx-scope": "off",
    "eol-last": "off",
    "linebreak-style": "off",
    "no-unused-vars": "warn",
    "import/no-unresolved":"off",
    "global-require":"off",
    "react/jsx-props-no-spreading":"off"
  }
}
